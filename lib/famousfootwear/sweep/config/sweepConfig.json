
{
	"store": "Famous Footwear",
	"baseURL": "https://www.famousfootwear.com",
	"pages": [{
		"category": "Athletic",
		"sweepType": "static",
		"entries": [
			"https://www.famousfootwear.com/en-US/Mens/_/_/Athletic+Shoes/On+Sale/Products.aspx"
		],
		"initSweep": {
			"sweepType": "static",
			"targetEl": ".productCellWrapper",
			"selectKeys": {
				"href": ".productImage>a",
				"brand": ".productBrandTitleColor .brand",
				"colors": ".productBrandTitleColor .color"
			}		
		}		
	}],
	"itemSweep": {
		"sweepType": "static",
		"selectKeys": {
			"name": ".PD_BrandStyle span[itemprop='name']",
			"origPrice": ".style-price span.origPrice",
			"curPrice": ".style-price span.price",
			"imgSrc": ".prodImg img[itemprop='image']",
			"availSize": ".input-select select.SizeWidth option",
			"details": "#ProductDescription li",
			"rating": "",
			"updatedDate": ""
		}
	},
	"fileWrite": {
		"path": "./data/"
	}

}





